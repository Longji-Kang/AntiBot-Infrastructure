{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::longji-definitions-storage-bucket/definitions/*"
        },
        {
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "${lambda_role}",
                    "${dev_role}",
                    "${admin_user}"
                ]
            },
            "Action": "s3*",
            "Resource": "arn:aws:s3:::longji-definitions-storage-bucket/definitions/*"
        }
    ]
}